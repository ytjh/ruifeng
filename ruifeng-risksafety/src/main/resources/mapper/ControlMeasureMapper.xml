<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ruifeng.mapper.ControlMeasureMapper">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.ruifeng.pojo.ControlMeasurePojo" id="controlMeasureMap">
        <result column="id"  property="id" />
        <result column="company_id"  property="companyId" />
        <result column="danger_id"  property="dangerId" />
        <result column="name"  property="name" />
        <result column="type"  property="type" />
        <result column="grade"  property="grade" />
        <result column="inspect_describe"  property="inspectDescribe" />
        <result column="inspect_describe_right"  property="inspectDescribeRight" />
        <result column="inspect_describe_error"  property="inspectDescribeError" />
        <result column="correct_term"  property="correctTerm" />
        <result column="check_rate"  property="checkRate" />
        <result column="del_flg"  property="delFlg" />
        <result column="remark"  property="remark" />
        <result column="create_by"  property="createBy" />
        <result column="create_time"  property="createTime" />
        <result column="update_by"  property="updateBy" />
        <result column="update_time"  property="updateTime" />
    </resultMap>
    
	<sql id="BaseList">
		id,
		company_id,
		danger_id,
		name,
		type,
		grade,
		inspect_describe,
		inspect_describe_right,
		inspect_describe_error,
		correct_term,
		check_rate,
		del_flg,
		remark,
		create_by,
		create_time,
		update_by,
		update_time
	</sql>
    
</mapper>